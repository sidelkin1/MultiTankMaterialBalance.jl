{
  "float": "Float64",
  "csv": {
    "dateformat": "dd.mm.yyyy",
    "delim": ";"
  },
  "solver": {
    "linalg": "recursive",
    "reorder": "amd",
    "maxiters": 10,
    "P_tol": 1e-7,
    "r_tol": 1e-5
  },
  "optimizer": {
    "package": "NLopt",
    "scaling": "sigmoid",
    "maxiters": 100,
    "NLopt": {
      "active": ["LD_LBFGS", "LD_TNEWTON_PRECOND_RESTART", "LD_MMA", "LD_CCSAQ", "LD_VAR2", "LD_VAR1"],
      "methods": {
        "LD_LBFGS": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_TNEWTON_PRECOND_RESTART": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_MMA": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_CCSAQ": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_SLSQP": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_VAR2": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        },
        "LD_VAR1": {
          "ftol_rel": 1E-12,
          "ftol_abs": 1E-12,
          "xtol_rel": 1E-12,
          "xtol_abs": 1E-12,
          "maxeval": 1000
        }
      }  
    },
    "SciPy": {
      "active": ["L-BFGS-B", "TNC"],
      "methods": {
        "L-BFGS-B": {
          "maxiter": 500,
          "disp": 0,
          "ftol": 1E-12,
          "maxfun": 1E10,
          "maxcor": 50,
          "maxls": 100
        },
        "SLSQP": {
          "maxiter": 500,
          "ftol": 1E-12,
          "disp": 0
        },
        "TNC": {
          "maxiter": 1000,
          "stepmx": 100,
          "ftol": 1E-12,
          "gtol": 1E-12,
          "xtol": 1E-12,
          "disp": 0
        },
        "trust-constr": {
          "maxiter": 500,
          "gtol": 1E-12,
          "xtol": 1E-12,
          "barrier_tol": 1E-12,
          "disp": 0
        }
      }
    }
  },
  "target_fun": {
    "alpha_resp": 1,
    "alpha_bhp": 0.01,
    "alpha_bhp_bound": 100,
    "alpha_inj": 0.01,
    "alpha_inj_bound": 100,
    "alpha_lb": 10,
    "alpha_ub": 10,
    "alpha_l2": 1
  }
}